//call back functiin 
#include<stdio.h>
void sum(int,int);
void sub(int, int);

void sum(int a, int b)
{
 printf("%d\n", a+b);
}
void sub(int a, int b)
{
 printf("%d\n", a-b);
 }
 void display(void (*ptr)(int, int))
 {
  ptr(10,20);
  
 }
 void main()
 {
  display(sum);
  display(sub);
  }
  
 Callback function
