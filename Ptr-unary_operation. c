#include<stdio.h>
void main()
{
  int a[10]={10,20,30,40,50}, c;
  int* p=&a;
  int* q=&a[4];
  printf("%d\n",*p++);
  printf("%d\n",*++p);
  printf("%d %d\n",*p++, *p++);// associativity from r->l
  printf("%d",*p);
  printf("decrement operation\n");
  printf("%d %d\n",*q--,*q--);
  printf("%d",*--q);
  }
  Pointer increment and decrement
