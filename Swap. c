#include<stdio.h>
void swap(int *n1,int *n2);

void main()
{
 int n1=10;
 int temp;
 int n2=20;
 
printf("Before::n1=%d && n2=%d\n", n1,n2);
swap(&n1,&n2);
/*temp=n1;
n1=n2;
n2=temp;*/
 printf("After::n1=%d && n2=%d", n1,n2);
}
void swap(int *n1,int *n2)
{
int temp;
 temp=*n1;
 *n1=*n2;
 *n2=temp;
}

